// Brute

class Solution {
public:
    void moveZeroes(vector<int>& nums) {
        int n = nums.size();
        int j = 0;
        for(int i = 0;i < n;i++){
            if(nums[i] == 0)
            {
                for(j = i+1;j < n;++j){
                    if(nums[j] != 0){
                        swap(nums[i], nums[j]);
                        break;
                    }
                }
            }
        }
    }
};

// Optimal
